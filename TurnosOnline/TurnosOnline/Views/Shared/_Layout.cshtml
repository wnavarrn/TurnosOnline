@*@using Codere.Web.RPP.Helpers*@
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <script src="~/Assets/jquery-ui-1.12.1.custom/external/jquery/jquery.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/jqueryval")*@
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datatable")
    @Scripts.Render("~/bundles/combobox")
    @Scripts.Render("~/bundles/bootbox")
    @Scripts.Render("~/bundles/toastr")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/custom")
    @Scripts.Render("~/bundles/switch")
    @*@Scripts.Render("~/bundles/animsition")*@
    @*@Scripts.Render("~/bundles/signature")*@

    @*<script src="~/Scripts/printThis.js"></script>*@

    @*<script src="~/Assets/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>*@

    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/datatable")
    @Styles.Render("~/Content/combobox")
    @Styles.Render("~/Content/toastr")
    @Styles.Render("~/Content/switch")

    <script src="~/Scripts/printThis.js"></script>
    <link href="~/Assets/FontAwesome/font-awesome.css" rel="stylesheet" />
    <script src="~/Scripts/loadingoverlay.js"></script>
    <link href="~/Assets/custom.css" rel="stylesheet" />
    @*<link rel="icon" href="@Url.Content("~/Assets/images/c_sola_codere.png")" />*@

    <script type="text/javascript">


        $(document).ready(function () {

            $(document).ajaxStart(function () {
                $.LoadingOverlay("show", {
                    zIndex: 9999
                });
            });
            $(document).ajaxStop(function () {
                $.LoadingOverlay("hide");
            });



        });
        //--------------------------------------

        @*</script>

    <script>*@




    </script>

</head>
<body>

    <div id="content">
        <header>
            <nav class="navbar navbar-expand-lg navbar-toggleable-md navbar-dark bg-white">
                <div class="container">
                    <a class="navbar-brand" rel="home" href="http://arg.codere.com/intranet/paginas/default.aspx" title="Centro medico las heras">
                        <img style="height:30px; margin-top: -11px;"
                             src="../Assets/images/logo.jpg">
                    </a>
                </div>
            </nav>
        
        </header>
        <div>         
            @RenderBody()
        </div>
        <footer class="page-footer font-small blue pt-4 mt-4">
            <!--Copyright-->
            <div class="footer-copyright py-3 text-center">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-12 align-self-end">     
                            <span class="text-muted">&copy;@DateTime.Now.Year - Todos los derechos reservados.</span>
                        </div>

                    </div>
                </div>
            </div>
            <!--/.Copyright-->

        </footer>
        @RenderSection("scripts", false)
    </div>

</body>


</html>
